
@model IEnumerable<CorporateWebSite__MVC.Models.Model.Hizmet>
    {
Layout=null;

}
<div class="col-lg-12">
    <h4 class="heading">Hizmetlerimiz</h4>
    <div class="row">
        <section id="projects">
            <ul id="thumbs" class="portfolio">

                @foreach (var item in Model)
                {
                    <!-- Item Project and Filter Name -->
                    <li class="col-lg-3 design" data-id="id-0" data-type="web">
                        <div class="item-thumbs">
                            <!-- Fancybox - Gallery Enabled - Title - Full Image -->
                            <a class="hover-wrap fancybox" data-fancybox-group="gallery" title="Work 1" href="img/works/1.jpg">
                                <span class="overlay-img"></span>
                                <span class="overlay-img-thumb font-icon-plus"></span>
                            </a>
                            <!-- Thumb Image and Description -->
                            <img src="@Model.hizmetResimURL" alt="@Model.hizmetBaslik">
                        </div>
                    </li>

                }


                <!-- End Item Project -->
            </ul>
        </section>
    </div>
</div>
